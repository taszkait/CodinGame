import java.util.*;
import java.io.*;
import java.math.*;

class Player {

    public static void main(String args[]) {
        Scanner in = new Scanner(System.in);
        
        while (true) {
            int mountain = 0;
            int mountainHigestIndex =0;
            int higestMountain = 0;
            
            for (int i = 0; i < 8; i++) {
                mountain = in.nextInt(); 
                
                if (mountain > higestMountain){
                    higestMountain = mountain;
                    mountainHigestIndex = i;
                }
            }
        System.out.println(mountainHigestIndex);
        }
    }
